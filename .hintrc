{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/time-and-media": [
      "default",
      {
        "meta-refresh": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ]
  }
}